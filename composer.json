{
    "name": "TiiToo/CrudGeneratorBundle",
    "description": "This bundle generates code cute for you",
    "keywords": ["crud", "generator", "bootstrap"],
    "homepage": "https://github.com/TiiToo/CrudGeneratorBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Gonzalo Alonso",
            "email": "gonkpo@gmail.com"
        }
    ],
    "require": {
        "symfony/symfony": "2.3.*",
        "sensio/generator-bundle": "2.3.*",
        "knplabs/knp-paginator-bundle": "2.3.*@dev",
        "lexik/form-filter-bundle": "1.2.*"
    },
    "autoload": {
        "psr-0": { "MWSimple\\Bundle\\CrudGeneratorBundle": "" }
    },
    "target-dir": "MWSimple/Bundle/CrudGeneratorBundle",
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "2.3.x-dev"
        }
    }
}
